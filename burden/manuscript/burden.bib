@article{lenaertExploringFeasibilityUsability2019,
  title = {Exploring the Feasibility and Usability of the Experience Sampling Method to Examine the Daily Lives of Patients with Acquired Brain Injury},
  author = {Lenaert, Bert and Colombi, Max and van Heugten, Caroline and Rasquin, Sascha and Kasanova, Zuzana and Ponds, Rudolf},
