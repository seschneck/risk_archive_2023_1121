  shorttitle = {Psychiatry {{Outpatients}}’ {{Willingness}} to {{Share Social Media Posts}} and {{Smartphone Data}} for {{Research}} and {{Clinical Purposes}}},
  author = {Rieger, Agnes and Gaines, Averi and Barnett, Ian and Baldassano, Claudia Frances and Connolly Gibbons, Mary Beth and Crits-Christoph, Paul},
  date = {2019-08-29},
  journaltitle = {JMIR Formative Research},
  shortjournal = {JMIR Form Res},
