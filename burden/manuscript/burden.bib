@article{jashinskyTrackingSuicideRisk2013,
  title = {Tracking {{Suicide Risk Factors Through Twitter}} in the {{US}}},
  author = {Jashinsky, Jared and Burton, Scott and Hanson, Carl and West, Josh and Giraud-Carrier, Christophe and Barnes, Michael and Argyle, Trenton},
