@article{dennisonOpportunitiesChallengesSmartphone2013,
  title = {Opportunities and {{Challenges}} for {{Smartphone Applications}} in {{Supporting Health Behavior Change}}: {{Qualitative Study}}},
  shorttitle = {Opportunities and {{Challenges}} for {{Smartphone Applications}} in {{Supporting Health Behavior Change}}},
