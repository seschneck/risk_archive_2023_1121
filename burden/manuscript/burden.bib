@article{kesslerGlobalBurdenMental2009,
  title = {The Global Burden of Mental Disorders: An Update from the {{WHO World Mental Health}} ({{WMH}}) Surveys},
  shorttitle = {The Global Burden of Mental Disorders},
  author = {Kessler, Ronald C and Aguilar-Gaxiola, Sergio and Alonso, Jordi and Chatterji, Somnath and Lee, Sing and Ormel, Johan and Ustün, T Bedirhan and Wang, Philip S},
