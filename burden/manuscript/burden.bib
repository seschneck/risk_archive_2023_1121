@article{spookMonitoringDietaryIntake2013,
  title = {Monitoring {{Dietary Intake}} and {{Physical Activity Electronically}}: {{Feasibility}}, {{Usability}}, and {{Ecological Validity}} of a {{Mobile}}-{{Based Ecological Momentary Assessment Tool}}},
  shorttitle = {Monitoring {{Dietary Intake}} and {{Physical Activity Electronically}}},
  author = {Spook, Jorinde Eline and Paulussen, Theo and Kok, Gerjo and Van Empelen, Pepijn},
