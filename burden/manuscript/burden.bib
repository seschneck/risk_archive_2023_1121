@article{morrenComplianceMomentaryPain2009,
  title = {Compliance with Momentary Pain Measurement Using Electronic Diaries: A Systematic Review},
  shorttitle = {Compliance with Momentary Pain Measurement Using Electronic Diaries},
  author = {Morren, Mattijn and van Dulmen, Sandra and Ouwerkerk, Jessika and Bensing, Jozien},
