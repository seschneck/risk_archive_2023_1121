@article{schomerusStigmaAlcoholDependence2011,
  title = {The {{Stigma}} of {{Alcohol Dependence Compared}} with {{Other Mental Disorders}}: {{A Review}} of {{Population Studies}}},
  shorttitle = {The {{Stigma}} of {{Alcohol Dependence Compared}} with {{Other Mental Disorders}}},
