@article{reeceForecastingOnsetCourse2017d,
  title = {Forecasting the Onset and Course of Mental Illness with {{Twitter}} Data},
  author = {Reece, Andrew G. and Reagan, Andrew J. and Lix, Katharina L. M. and Dodds, Peter Sheridan and Danforth, Christopher M. and Langer, Ellen J.},
  date = {2017-10-11},
  journaltitle = {Scientific Reports},
  volume = {7},
