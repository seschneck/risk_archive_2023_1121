@article{dennisonOpportunitiesChallengesSmartphone2013,
  title = {Opportunities and {{Challenges}} for {{Smartphone Applications}} in {{Supporting Health Behavior Change}}: {{Qualitative Study}}},
  shorttitle = {Opportunities and {{Challenges}} for {{Smartphone Applications}} in {{Supporting Health Behavior Change}}},
  author = {Dennison, Laura and Morrison, Leanne and Conway, Gemma and Yardley, Lucy},
  date = {2013},
  journaltitle = {Journal of Medical Internet Research},
  volume = {15},
  number = {4},
  pages = {e86},
  publisher = {{JMIR Publications Inc., Toronto, Canada}},
  doi = {10.2196/jmir.2583},
  url = {https://www.jmir.org/2013/4/e86/},
  urldate = {2021-02-12},
  abstract = {Background: There is increasing interest from academics and clinicians in harnessing smartphone applications (apps) as a means of delivering behavioral interventions for health.  Despite the growing availability of a range of health-related apps on the market, academic research on the development and evaluation of such apps is in the relatively early stages. A few existing studies have explored the views of various populations on using mobile phones for health-related issues and some studies are beginning to report user feedback on specific apps.  However, there remains little in depth research on users\&\#8217; (and potential users\&\#8217;) experiences and views on a wide range of features and technologies that apps are, or will soon be, capable of.  In particular, research on young adults is lacking, which is an unfortunate omission considering that this group comprises of a good number of mobile technology adoptors. Objective: The current study sought to explore young adults\&\#8217; perspectives on apps related to health behavior change. It sought their experiences and views of features that might support health behavior change and issues that contribute to interest in and willingness to use such apps. Methods: Four focus groups were conducted with 19 students and staff at a University in the United Kingdom. Participants included 13 females and 6 males with a mean age of 23.79 (SD 7.89). The focus group discussions centred on participants\&\#8217; experiences of using smartphone apps to support a healthy lifestyle, and their interest in and feelings about features and capabilities of such apps. The focus groups were recorded, transcribed, and analyzed using inductive thematic analysis. Results: Study findings suggested that young, currently healthy adults have some interest in apps that attempt to support health-related behavior change. Accuracy and legitimacy, security, effort required, and immediate effects on mood emerged as important influences on app usage. The ability to record and track behavior and goals and the ability to acquire advice and information \&\#8220;on the go\&\#8221; were valued. Context-sensing capabilities and social media features tended to be considered unnecessary and off-putting. Conclusions: This study provided insight into the opportunities and challenges involved in delivering health-related behavioral interventions through smartphone apps. The findings suggested a number of valued features and characteristics that app developers may wish to consider when creating health behavior apps. Findings also highlighted several major challenges that appeared to need further consideration and research to ensure the development of effective and well-accepted behavior change apps.  [J Med Internet Res 2013;15(4):e86]},
  langid = {english},
  file = {C\:\\Users\\jjcurtin\\Zotero\\storage\\ZUYG3D7W\\Dennison et al. - 2013 - Opportunities and Challenges for Smartphone Applic.pdf;C\:\\Users\\jjcurtin\\Zotero\\storage\\R374UGHQ\\e86.html}
}

