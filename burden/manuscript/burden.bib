  file = {C\:\\Users\\jjcurtin\\Zotero\\storage\\7TUQINIF\\Obermeyer et al. - 2019 - Dissecting racial bias in an algorithm used to man.pdf;C\:\\Users\\jjcurtin\\Zotero\\storage\\JTL2LXPV\\tab-pdf.html}
}

