  title = {Perceived Barriers to Psychological Treatments and Their Relationship to Depression},
  author = {Mohr, David C. and Ho, Joyce and Duffecy, Jenna and Baron, Kelly G. and Lehman, Kenneth A. and Jin, Ling and Reifler, Douglas},
