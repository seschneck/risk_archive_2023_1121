@article{satianiProjectedWorkforcePsychiatrists2018,
  title = {Projected {{Workforce}} of {{Psychiatrists}} in the {{United States}}: {{A Population Analysis}}},
  shorttitle = {Projected {{Workforce}} of {{Psychiatrists}} in the {{United States}}},
  author = {Satiani, Anand and Niedermier, Julie and Satiani, Bhagwan and Svendsen, Dale P.},
  year = {2018},
