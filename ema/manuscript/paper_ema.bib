@article{gottliebMachineLearningPredicting2022,
  title = {Machine {{Learning}} for {{Predicting Risk}} of {{Early Dropout}} in a {{Recovery Program}} for {{Opioid Use Disorder}}},
  author = {Gottlieb, Assaf and Yatsco, Andrea and {Bakos-Block}, Christine and Langabeer, James R. and {Champagne-Langabeer}, Tiffany},
  year = {2022},
