@article{ngUserEngagementMental2019,
  title = {User {{Engagement}} in {{Mental Health Apps}}: {{A Review}} of {{Measurement}}, {{Reporting}}, and {{Validity}}},
  shorttitle = {User {{Engagement}} in {{Mental Health Apps}}},
  author = {Ng, Michelle M. and Firth, Joseph and Minen, Mia and Torous, John},
  year = {2019},
